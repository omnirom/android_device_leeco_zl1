/boot            emmc              /dev/block/bootdevice/by-name/boot flags=backup=1
/system          ext4              /dev/block/bootdevice/by-name/system
/data            ext4              /dev/block/bootdevice/by-name/userdata  flags=encryptable=footer;length=-16384	 
/cache	         ext4              /dev/block/bootdevice/by-name/cache
/recovery        emmc              /dev/block/bootdevice/by-name/recovery   flags=backup=1
/modem           emmc	           /dev/block/bootdevice/by-name/modem	flags=backup=1;display="EFS"
/efs1            emmc              /dev/block/bootdevice/by-name/modemst1  flags=backup=1;subpartitionof=/modem
/efs2            emmc              /dev/block/bootdevice/by-name/modemst2  flags=backup=1;subpartitionof=/modem
/bluetooth       emmc	           /dev/block/bootdevice/by-name/bluetooth	flags=backup=1;subpartitionof=/modem
/dsp             emmc	           /dev/block/bootdevice/by-name/dsp	flags=backup=1;subpartitionof=/modem
/persist	 ext4              /dev/block/bootdevice/by-name/persist	flags=display="Persist"

/usb-otg         vfat              /dev/block/sdg1      /dev/block/sdg              flags=removable;storage;display=USB-OTG
